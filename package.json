{
  "name": "mobiledeck",
  "displayName": "Mobile Deck",
  "description": "View, control and manage all your Android and iOS devices, simulators, and emulators without leaving your IDE",
  "version": "0.0.1",
  "publisher": "mobilenext",
  "author": {
    "name": "Mobile Next"
  },
  "license": "AGPL-3.0",
  "icon": "mobilenexthq_logo.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "bugs": {
    "url": "https://github.com/mobile-next/mobiledeck/issues"
  },
  "homepage": "https://github.com/mobile-next/mobiledeck#readme",
  "categories": [
    "Testing"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mobile-next/mobiledeck"
  },
  "activationEvents": [
    "onView:mobiledeckDevices"
  ],
  "main": "./out/extension.js",
  "private": true,
  "contributes": {
    "commands": [
      {
        "command": "mobiledeck.connect",
        "title": "Connect",
        "icon": "$(device-mobile)"
      },
      {
        "command": "mobiledeck.openDevicePanel",
        "title": "Open Device Panel"
      },
      {
        "command": "mobiledeck.signOut",
        "title": "Sign Out",
        "enablement": "mobiledeck.isAuthenticated"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "mobiledeck.signOut",
          "when": "view == mobiledeckDevices && mobiledeck.isAuthenticated"
        }
      ]
    },
    "views": {
      "mobiledeck": [
        {
          "id": "mobiledeckDevices",
          "name": "Devices",
          "type": "webview",
          "when": "true"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mobiledeck",
          "title": "Mobiledeck",
          "icon": "mobiledeck-icon.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "prepare": "npx husky",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^9.23.0",
    "husky": "<=9.1.7",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@mobilenext/mobilecli": "0.0.28",
    "oidc-client-ts": "^3.3.0",
    "react-icons": "5.5.0"
  },
  "keywords": [],
  "type": "commonjs"
}
