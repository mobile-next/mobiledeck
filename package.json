{
  "name": "mobiledeck",
  "displayName": "mobiledeck",
  "description": "",
  "version": "0.0.1",
  "publisher": "mobile-next",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mobile-next/mobiledeck"
  },
  "activationEvents": [
    "onView:mobiledeckSidebar",
    "onCommand:mobiledeck.openPreview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mobiledeck.openPreview",
        "title": "Mobiledeck: View Device"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mobiledeckActivityBar",
          "title": "Mobiledeck",
          "icon": "assets/ios.svg"
        }
      ]
    },
    "views": {
      "mobiledeckActivityBar": [
        {
          "type": "webview",
          "id": "mobiledeckSidebar",
          "name": ""
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "prepare": "npx husky",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "husky": "<=9.1.7",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "vsce": "^2.15.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@mobilenext/mobilecli": "0.0.9",
    "react-icons": "^5.5.0"
  }
}
