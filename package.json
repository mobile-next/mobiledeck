{
  "name": "mobiledeck",
  "displayName": "Mobile Deck",
  "description": "View, control and manage all your Android and iOS devices, simulators, and emulators",
  "version": "0.0.1",
  "publisher": "mobilenext",
  "author": "Mobile Next",
  "license": "AGPL-3.0",
  "icon": "media/mobilenexthq_logo.png",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Debuggers", "Testing"
  ],
  "keywords": [
    "mobile", "xcode", "adb", "emulator", "simulator", "ios", "android"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mobile-next/mobiledeck.git"
  },
  "activationEvents": [
    "onView:mobiledeckDevices"
  ],
  "bugs": {
    "url": "https://github.com/mobile-next/mobiledeck/issues"
  },
  "homepage": "https://github.com/mobile-next/mobiledeck/blob/main/README.md",
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mobiledeck.refresh",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "mobiledeck.addDevice",
        "title": "Add Device",
        "icon": "$(plus)"
      },
      {
        "command": "mobiledeck.connect",
        "title": "Connect",
        "icon": "$(device-mobile)"
      },
      {
        "command": "mobiledeck.openDevicePanel",
        "title": "Open Device Panel"
      }
    ],
    "views": {
      "mobiledeck": [
        {
          "id": "mobiledeckDevices",
          "name": "Devices",
          "when": "true"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "mobiledeck",
          "title": "Mobiledeck",
          "icon": "media/mobiledeck.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "mobiledeck.refresh",
          "when": "view == mobiledeckDevices",
          "group": "navigation"
        },
        {
          "command": "mobiledeck.addDevice",
          "when": "view == mobiledeckDevices",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "mobiledeck.connect",
          "when": "view == mobiledeckDevices && viewItem == device",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "prepare": "npx husky",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "3.6.2",
    "eslint": "^9.23.0",
    "husky": "<=9.1.7",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "react-icons": "^5.5.0",
    "@mobilenext/mobilecli": "0.0.27"
  },
  "type": "commonjs"
}
